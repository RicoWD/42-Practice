char	*ft_strstr(char *str, char *to_find)
{
	int	i;
	int	p;

	i = 0;
	p = 0;
	while (str[i])
	{
		if (str[i] = str[j])
		{
			if (!str[j])
			{
				p = str[i]; 
			}
			i++;
			j++;
		}
	}
	return p;
}


int	main(void)
{
	char	haystack[] = "Voici le panel, de tous les caract√®res";
	char	needle[] = "les";

